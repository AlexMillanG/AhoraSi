ROLS
1.superAdmin
2 admin
3 users

STATUS

 // vista para mostrar las historias del usuario
create view showStoriesByUser as

    select users.name_,
           stories.user_id,
           users.lastname ,
           users.surname,
           stories.title,
           stories.content
      from users
    inner join stories on users.id = stories.user_id;
